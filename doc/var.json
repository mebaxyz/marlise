{
  "MOD_DEV_ENVIRONMENT": {
    "default": false,
    "description": "Enables development environment mode."
  },
  "MOD_DEV_HMI": {
    "default": "MOD_DEV_ENVIRONMENT",
    "description": "Enables development mode for the HMI (Human-Machine Interface)."
  },
  "MOD_DEV_HOST": {
    "default": "MOD_DEV_ENVIRONMENT",
    "description": "Enables development mode for the host system."
  },
  "MOD_DEV_API": {
    "default": false,
    "description": "Uses development cloud API environment instead of production."
  },
  "MOD_DESKTOP": {
    "default": false,
    "description": "Indicates if running in desktop mode."
  },
  "MOD_LOG": {
    "default": 0,
    "description": "Logging verbosity level."
  },
  "MOD_API_KEY": {
    "default": null,
    "description": "API authentication key."
  },
  "MOD_DEVICE_KEY": {
    "default": null,
    "description": "Device authentication key."
  },
  "MOD_DEVICE_TAG": {
    "default": null,
    "description": "Device tag identifier."
  },
  "MOD_DEVICE_UID": {
    "default": null,
    "description": "Device unique identifier."
  },
  "MOD_IMAGE_VERSION_PATH": {
    "default": "/etc/mod-release/release",
    "description": "Path to file containing the image version."
  },
  "MOD_HARDWARE_DESC_FILE": {
    "default": "/etc/mod-hardware-descriptor.json",
    "description": "Path to hardware descriptor JSON file."
  },
  "MOD_DATA_DIR": {
    "default": "~/data",
    "description": "Base directory for data storage."
  },
  "MOD_USER_FILES_DIR": {
    "default": "/data/user-files",
    "description": "Directory for user files."
  },
  "MOD_KEYS_PATH": {
    "default": "<DATA_DIR>/keys",
    "description": "Path to keys directory (must end with '/')."
  },
  "MOD_FAVORITES_JSON": {
    "default": "<DATA_DIR>/favorites.json",
    "description": "Path to favorites JSON file."
  },
  "MOD_LAST_STATE_JSON": {
    "default": "<DATA_DIR>/last.json",
    "description": "Path to last state JSON file."
  },
  "MOD_PREFERENCES_JSON": {
    "default": "<DATA_DIR>/prefs.json",
    "description": "Path to preferences JSON file."
  },
  "MOD_USER_ID_JSON": {
    "default": "<DATA_DIR>/user-id.json",
    "description": "Path to user ID JSON file."
  },
  "MOD_USER_BANKS_JSON": {
    "default": "<DATA_DIR>/banks.json",
    "description": "Path to user banks JSON file."
  },
  "MOD_FACTORY_BANKS_JSON": {
    "default": "/usr/share/mod/banks.json",
    "description": "Path to factory banks JSON file."
  },
  "MOD_DOWNLOAD_TMP_DIR": {
    "default": "/tmp/mod-ui",
    "description": "Temporary directory for downloads."
  },
  "MOD_PEDALBOARD_TMP_DIR": {
    "default": "<DATA_DIR>/pedalboard-tmp-data",
    "description": "Temporary directory for pedalboard data."
  },
  "MOD_USER_PLUGINS_DIR": {
    "default": "~/.lv2",
    "description": "Directory for user LV2 plugins."
  },
  "MOD_USER_PEDALBOARDS_DIR": {
    "default": "~/.pedalboards",
    "description": "Directory for user pedalboards."
  },
  "MOD_FACTORY_PEDALBOARDS_DIR": {
    "default": "/usr/share/mod/pedalboards",
    "description": "Directory for factory pedalboards."
  },
  "MOD_HMI_BAUD_RATE": {
    "default": 10000000,
    "description": "Baud rate for HMI serial communication."
  },
  "MOD_HMI_SERIAL_PORT": {
    "default": "/dev/ttyUSB0",
    "description": "Serial port for HMI connection."
  },
  "MOD_HMI_TIMEOUT": {
    "default": 0,
    "description": "Timeout for HMI communication."
  },
  "MOD_MODEL_CPU": {
    "default": null,
    "description": "CPU model identifier."
  },
  "MOD_MODEL_TYPE": {
    "default": null,
    "description": "Device model type."
  },
  "MOD_DEVICE_WEBSERVER_PORT": {
    "default": 80,
    "description": "Port for device web server."
  },
  "MOD_DEVICE_HOST_PORT": {
    "default": 5555,
    "description": "Port for device host communication."
  },
  "MOD_HTML_DIR": {
    "default": "<sys.prefix>/share/mod/html/",
    "description": "Directory for HTML resources."
  },
  "MOD_DEFAULT_PEDALBOARD": {
    "default": "<sys.prefix>/share/mod/default.pedalboard",
    "description": "Path to default pedalboard file."
  },
  "MOD_CLOUD_HTTP_ADDRESS": {
    "default": "https://api.mod.audio/v2",
    "description": "Cloud API base URL."
  },
  "MOD_CLOUD_LABS_HTTP_ADDRESS": {
    "default": "https://api-labs.mod.audio/v2",
    "description": "Cloud Labs API base URL."
  },
  "MOD_PLUGINS_HTTP_ADDRESS": {
    "default": "https://pedalboards.mod.audio/plugins",
    "description": "Plugins API URL."
  },
  "MOD_PEDALBOARDS_HTTP_ADDRESS": {
    "default": "https://pedalboards.mod.audio",
    "description": "Pedalboards API URL."
  },
  "MOD_PEDALBOARDS_LABS_HTTP_ADDRESS": {
    "default": "https://pedalboards-labs.mod.audio",
    "description": "Pedalboards Labs API URL."
  },
  "MOD_CONTROLCHAIN_HTTP_ADDRESS": {
    "default": "https://download.mod.audio/releases/cc-firmware/v3",
    "description": "ControlChain firmware download URL."
  },
  "MOD_TUNER_PLUGIN": {
    "default": "gxtuner",
    "description": "Tuner plugin to use ('gxtuner' or 'tuna')."
  },
  "MOD_UPDATE_MOD_OS_FILE": {
    "default": "modduo.tar",
    "description": "Filename for MOD OS update package (stored in /data/)."
  }
}
